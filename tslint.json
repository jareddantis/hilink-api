{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": false,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": false,
    "no-string-literal": false,
    "prefer-const": false,
    "quotemark": [false],
    "semicolon": [true, "never"]
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}
